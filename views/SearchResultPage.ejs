<html>
<head>
        <meta charset="utf-8">
	<title>Search Result Page</title>
</head>

<body>
	<div>
       	       <h1>Showing <%=restaurant.length%> document(s).</h1>
        </div>

		<ol>
			<% for (i in restaurant) { %>
				<li>Name: <a href=/display?_id=<%= restaurant[i]._id %>><%= restaurant[i].name %></a></li>
			<% } %>
		</ol>

	<div>
	<button class="w3-btn w3-section w3-teal w3-ripple" onclick="goBack()">Go Back</button>
	</div>

	<script>
            function goBack() {window.history.back();}
        </script>

</body>
</html>
